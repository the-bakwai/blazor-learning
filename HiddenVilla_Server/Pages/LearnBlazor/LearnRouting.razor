@page "/learnrouting"
@page "/newrouting"
@page "/learnrouting/{parameter1}"
@page "/learnrouting/{parameter1}/{parameter2}"
<h3>LearnRouting</h3>
<h4>Parameter 1: @Parameter1</h4>
<h4>Parameter 2: @Parameter2</h4>

<NavLink class="btn btn-secondary" href="learnadvancedrouting?Para1=steve&Param2=Blazor">
    <span class="oi oi-list-rich"></span> Routing with query param
</NavLink>


@code {
    [Parameter]
    public string Parameter1 { get; set; }
    [Parameter]
    public string Parameter2 { get; set; }
}